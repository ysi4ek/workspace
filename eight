package type8;
public class type8 {
	    public static void main(String[] args) {
	        int n = 5;
	        int i;
	        boolean Compositenumber = false;
	        for (i = 2; i < n; i++) {
	            if (n % i == 0) {
	                Compositenumber = true;
	                break;
	            }
	        }
	        if (Compositenumber) {
	            System.out.println("число составное ");
	        } else {
	            System.out.println("число  простое ");
	        }
	    }
}
